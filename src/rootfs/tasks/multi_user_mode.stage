name = "MultiUserMode"
description = "Multi User Mode + Networking. Basically init 3"
steps = [
    { name = "SingleUserMode" },
    { name = "getty", args = {"tty" = "tty1"}},
    { name = "freudian", args = {"socketpath" = "/run/freudian/socket"}},
    { name = "qdevd", requirements = [
        { name = "freudian" }
    ]}
]
